{
	"model": {
		"architecture": "resnet18",
		"target_classes": 10
	},
	"train_parameters": {
		"lr": 0.001,
		"batch_size": 1000,
		"n_epoch": 80,
		"max_accuracy": 0.99,
		"isreset": true,
		"islogs":  true,
		"logs_location": "./logs.txt"
	},

	"active_learning":{
		"strategy": "random_sampling",
		"budget": 1000,
		"rounds": 15,
		"initial_points":1000,
		
		"strategy_args":{	
			"batch_size" : 1000, 
			"lr":0.001
		}
	},
	"dataset":{
		"name":"cifar10"
	}
}